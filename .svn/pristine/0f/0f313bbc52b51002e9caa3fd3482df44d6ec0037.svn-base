<VirtualHost *:80>
    ServerName lts.brandeis.edu
    ServerAlias lts
    DocumentRoot /u1/www/lts
    Include /var/applications/contentserv/production/conf/bens-alert.conf
    ServerAdmin webmaster@brandeis.edu
    AddHandler server-parsed .shtml .html
    ErrorDocument 404 /404.html
    Include /var/applications/contentserv/production/conf/lts-redirects.conf
    <Directory "/u1/www/lts">
          Options -Indexes FollowSymLinks IncludesNoExec
        AllowOverride AuthConfig Limit FileInfo
    </Directory>
    <Directory "/u1/www/lts/staff">
        php_flag engine on
	DirectoryIndex index.html index.htm index.shtml index.php3 index.php
    </Directory>
    <Directory "/u1/www/lts/apps">
        php_flag engine on
   	DirectoryIndex index.html index.htm index.shtml index.php3 index.php
    </Directory>
    <Directory "/u1/www/lts/includes">
        Options Includes FollowSymLinks
   	php_flag engine on
    </Directory>
    php_flag engine off
    <Files ~ "(^\.ht|\.con$|\.pwd$|error_log|access_log|\.inc$|\.cf$|~|\.old$|\.bak$)">
        Order allow,deny
        Deny from all
    </Files>

   RewriteEngine On
      RewriteCond %{HTTPS} off
         RewriteRule ^(.*)$ https://%{SERVER_NAME}$1 [R=301,L]

</VirtualHost>
